{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules":{
    "no-console":false,
    "no-debugger":false,
    "indent":[true, "spaces", 2], // 缩进
    "no-consecutive-blank-lines": [// 空行不超过两行
      true,
      2
    ],
    "jsx-no-lambda": false,  
    "interface-name": false,  
    // 检查对象中键的排序
    "object-literal-sort-keys": false,
    // 倒入（import）源的排序规则
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
